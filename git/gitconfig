# This is Git's per-user configuration file.
[user]
	name = Rogan Creswick
	email = rogan@groq.com
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge --skip %f
	process = git-lfs filter-process --skip
	required = true
[core]
	editor = /usr/bin/vim
	excludesFile = ~/.gitignore

[alias]
        lol = log --oneline --graph
[rerere]
        enabled = true
